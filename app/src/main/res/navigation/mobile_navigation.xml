<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.Fujairah.convertgeneration.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.Fujairah.convertgeneration.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile"/>
    <fragment
        android:id="@+id/nav_view_attendance"
        android:name="com.Fujairah.convertgeneration.ui.view.ViewAttendanceFragment"
        android:label="@string/view_attendance"
        tools:layout="@layout/fragment_view_attendance"/>

    <dialog
        android:id="@+id/nav_logout"
        android:name="com.Fujairah.convertgeneration.dialogs.LogoutDialog"
        android:label="@string/menu_logout"
        tools:layout="@layout/dialog_logout" />



</navigation>